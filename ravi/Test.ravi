module List =

  let empty = []
  end

  let ( @ ) l1 l2 = concat l1 l2
  end

  let isEmpty l = match l with
    | [] -> "true"
    | _ -> "false"
  end

  let map f l =
    match l with
    | [] -> []
    | h :: t -> (f h) :: map f t
  end

end

