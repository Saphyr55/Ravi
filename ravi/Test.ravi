module List =

  let empty = []
  end

  let ( @ ) l1 l2 = concat l1 l2
  end

  let isEmpty l =
    match l with
    | [] -> 1
    | _ -> 0
  end

  let map f l =
    match l with
    | [] -> []
    | h :: t -> (f h) :: map f t
  end

  let count l =
    let aux l n =
      match l with
      | [] -> n
      | _ :: t -> aux t (n + (neg 1))
    in
    aux l 0
  end

end

